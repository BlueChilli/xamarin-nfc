<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Plugin.Nfc</id>
        <version>1.4.4</version>
        <title>Nfc Plugin for Xamarin</title>
        <authors>Sven-Michael Stübe</authors>
        <owners>Sven-Michael Stübe</owners>
        <licenseUrl>https://github.com/smstuebe/xamarin-nfc/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/smstuebe/xamarin-nfc</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Read NFC tags from a cross platform API.</description>
        <summary>Read NFC tags from a cross platform API.</summary>
        <tags>xamarin monodroid Xamarin.iOS10 mac Xamarin.Mac ios nfc ndef Xamarin.Android android uwp</tags>
        <iconUrl>https://raw.githubusercontent.com/smstuebe/xamarin-nfc/master/doc/xamarin_nfc.png</iconUrl>
        <releaseNotes>

        </releaseNotes>
        <dependencies>
            <group targetFramework="net">
            </group>
            <group targetFramework="win">
            </group>
            <group targetFramework="wp">
            </group>
            <group targetFramework="wpa">
            </group>
            <group targetFramework="netstandard1.0">
                <dependency id="NETStandard.Library" version="1.6.1" />
            </group>
            <group targetFramework="MonoAndroid10">
            </group>
            <group targetFramework="Xamarin.iOS10">
            </group>
            <group targetFramework="Xamarin.Mac20">
            </group>
            <group targetFramework="portable-net45+win+wpa81+wp8">
            </group>
            <group targetFramework="uap">
            </group>
            <group targetFramework="dotnet">
            </group>
            <group targetFramework="xamarintvos">
            </group>
            <group targetFramework="xamarinwatchos">
            </group>
        </dependencies>
    </metadata>
    <files>
        <!-- PCL / net standard -->
        <file src="pcl\Plugin.Nfc.*" target="lib\portable-net45+win8+wpa81+wp8" />
        <file src="pcl\Plugin.Nfc.*" target="lib\netstandard1.0" />
        <!-- .NET 4.5 -->
        <file src="null\Plugin.Nfc.*" target="lib\net45" />
        <!-- Android -->
        <file src="android\Plugin.Nfc.*" target="lib\MonoAndroid" />
        <!-- iOS -->
        <file src="ios\Plugin.Nfc.*" target="lib\Xamarin.iOS10" />
        <!-- Mac -->
        <file src="mac\Plugin.Nfc.*" target="lib\Xamarin.Mac20" />
        <!-- UWP -->
        <file src="uwp\Plugin.Nfc.*" target="lib\UAP10" />
        <file src="uwp\Plugin.Nfc\Properties\*.rd.xml" target="lib\UAP10\Plugin.Nfc\Properties" />
    </files>
</package>